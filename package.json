{
  "name": "vscode-extension-test",
  "displayName": "vscode-extension-test",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai.selectCode",
        "title": "AI-解释代码"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "ai.selectCode",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vs-sidebar-view",
          "title": "AI",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "vs-sidebar-view": [
        {
          "type": "webview",
          "id": "vs-sidebar-view",
          "name": "你的专属AI助手"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "build": "pnpm exec vsce package --no-dependencies"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.7",
    "@types/cross-spawn": "~6.0.2",
    "@types/detect-port": "~1.3.2",
    "@types/fs-extra": "~9.0.13",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.11.45",
    "@types/react": "^16.8.0",
    "@types/react-dom": "16.9.14",
    "@types/semver": "~7.3.13",
    "@types/styled-components": "~5.1.26",
    "@types/vscode": "^1.57.1",
    "@vscode/test-electron": "^2.2.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "fork-ts-checker-webpack-plugin": "~7.3.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "~5.5.0",
    "mocha": "^10.1.0",
    "style-loader": "^3.3.1",
    "typescript": "4.3.4",
    "url-loader": "~4.1.1",
    "vsce": "~2.15.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.0.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "antd": "^5.8.5",
    "axios": "^1.4.0",
    "handlebars": "^4.7.7",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-markdown": "^8.0.7",
    "styled-components": "~5.3.6"
  }
}
